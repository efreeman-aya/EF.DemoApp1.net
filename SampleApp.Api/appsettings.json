{
  "TodoServiceSettings": {
    "StringValue": "some string",
    "IntValue": 5
  },
  "ScheduledServiceSettings": {
    "CronServices": [
      {
        "ServiceName": "Run every 30 seconds",
        "Schedule": "*/30 * * * * *",
        "SleepIntervalSeconds": 5,
        "SomeUrl": "some url"
      },
      {
        "ServiceName": "Run every 1 minute",
        "Schedule": "0 */1 * * * *",
        "SleepIntervalSeconds": 10,
        "SomeTopicOrQueue": "some topic"
      },
      {
        "ServiceName": "Run every 2 minutes",
        "Schedule": "0 */2 * * * *",
        "SleepIntervalSeconds": 20,
        "SomeUrl": "some url"
      }
    ]
  },
  "Logging": {
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft": "Error",
        "System": "Warning"
      }
    },
    "EnableSqlCommandTextInstrumentation": true,
    //default logger
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  },
  "AllowedHosts": "*",
  //convention picked up by app insights
  "ApplicationInsights": {
    "InstrumentationKey": "",
    "EnableAdaptiveSampling": false,
    "EnablePerformanceCounterCollectionModule": false
  }
}
