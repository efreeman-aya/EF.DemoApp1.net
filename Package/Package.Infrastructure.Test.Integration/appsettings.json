{
  "AzureClientDefaults": {
    "Diagnostics": {
      "IsTelemetryDisabled": false,
      "IsLoggingContentEnabled": true
    },
    "Retry": {
      "MaxRetries": 5,
      "Delay": 3,
      "Mode": "Exponential"
    }
  },
  "AzureBlobStorageManagerSettings": {
  },
  "EventGridPublisherManagerSettings": {
    "LogEventData": true
  },
  "AzureBlobStorageAccount1": {
    "ServiceUri": "http://127.0.0.1:10000/devstoreaccount1"
  },
  "Logging": {
    //default logger
    "LogLevel": {
      "Default": "Debug",
      "System": "Warning",
      "Microsoft": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  //convention picked up by app insights
  "ApplicationInsights": {
    "InstrumentationKey": "",
    "EnableAdaptiveSampling": false,
    "EnablePerformanceCounterCollectionModule": false
  }
}
