{
  "AzureBlobStorageManagerSettings": {
    "RefreshIntervalSeconds": 590
  },
  "AzureBlobStorageAccount1": {
    "Url": "http://127.0.0.1:10000/devstoreaccount1"
  },
  "Logging": {
    //default logger
    "LogLevel": {
      "Default": "Debug",
      "System": "Warning",
      "Microsoft": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  //convention picked up by app insights
  "ApplicationInsights": {
    "InstrumentationKey": "",
    "EnableAdaptiveSampling": false,
    "EnablePerformanceCounterCollectionModule": false
  }
}
